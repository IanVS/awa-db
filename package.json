{
  "name": "awa-db",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro preview",
    "build": "astro build",
    "preview": "astro build && astro preview",
    "astro": "astro",
    "deploy": "gh-pages -d dist",
    "generate-favicons": "real-favicon generate favicon.config.json faviconData.json ./public",
    "prepare": "husky install",
    "lint": "eslint --fix \"**/*.{ts,tsx,js,cjs,mjs,jsx,astro}\"",
    "lint:styles": "npx stylelint --fix \"**/*.css\"",
    "commit": "cz"
  },
  "devDependencies": {
    "@astrojs/image": "^0.16.5",
    "@astrojs/sitemap": "^1.2.2",
    "@astrojs/tailwind": "^3.1.1",
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-conventional": "^17.4.4",
    "@jasikpark/astro-svg-loader": "^0.1.0",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "astro": "^2.2.1",
    "astro-compress": "^1.1.35",
    "astro-critters": "^1.1.31",
    "cli-real-favicon": "^0.0.8",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-astro": "^0.26.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "gh-pages": "^5.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.7",
    "prettier-plugin-astro": "^0.8.0",
    "prettier-plugin-tailwindcss": "^0.2.7",
    "prettier-stylelint": "^0.4.2",
    "sharp": "^0.31.3",
    "stylelint": "^15.4.0",
    "stylelint-config-idiomatic-order": "^9.0.0",
    "stylelint-config-standard": "^32.0.0",
    "stylelint-config-tailwindcss": "^0.0.7",
    "tailwind-scrollbar-hide": "^1.1.7",
    "tailwindcss": "^3.3.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "sideEffects": [
    "**/*.css"
  ],
  "engines": {
    "node": ">=18"
  }
}
